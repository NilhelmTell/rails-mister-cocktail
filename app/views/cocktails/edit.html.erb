<h1>change your cocktail</h1>

<div class="container">
    <%= simple_form_for(@cocktail) do |f| %>
    <%= f.input :name, placeholder: "change" %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
    <% end %>
</div>

<%= link_to 'Show', cocktail_path(@cocktail) %> |
<%= link_to 'Back to cocktail', cocktails_path %>
